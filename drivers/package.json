{
    "name": "@llumiverse/drivers",
    "version": "0.8.0",
    "type": "module",
    "description": "LLM driver implementations. Currently supported are: openai, huggingface, bedrock, replicate.",
    "files": [
        "lib",
        "src"
    ],
    "types": "./lib/types/index.d.ts",
    "exports": {
        "types": "./lib/types/index.d.ts",
        "import": "./lib/esm/index.js",
        "require": "./lib/cjs/index.js",
        "default": "./lib/esm/index.js"
    },
    "scripts": {
        "test": "ESBK_TSCONFIG_PATH=./test/tsconfig.json npx mocha -r tsx ./test/**/*.ts",
        "build": "npm run clean && tsc -p ./tsconfig.esm.json && tsc -p ./tsconfig.cjs.json",
        "clean": "rm -rf ./lib tsconfig.tsbuildinfo"
    },
    "license": "MIT",
    "devDependencies": {
        "@types/bun": "^1.0.4",
        "@types/chai": "^4.3.11",
        "@types/estree": "^1.0.1",
        "@types/eventsource": "^1.1.15",
        "@types/json-schema": "^7.0.15",
        "@types/mocha": "^10.0.1",
        "chai": "^4.3.8",
        "dotenv": "^16.4.1",
        "mocha": "^10.2.0",
        "tsx": "^3.12.10",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@aws-sdk/client-bedrock": "^3.451.0",
        "@aws-sdk/client-bedrock-runtime": "^3.451.0",
        "@aws-sdk/client-s3": "^3.496.0",
        "@aws-sdk/credential-providers": "^3.451.0",
        "@aws-sdk/lib-storage": "^3.496.0",
        "@google-cloud/aiplatform": "^3.10.0",
        "@google-cloud/vertexai": "^0.2.1",
        "@huggingface/inference": "^2.6.4",
        "@llumiverse/core": "*",
        "api-fetch-client": "^0.8.6",
        "eventsource": "^2.0.2",
        "google-auth-library": "^9.6.1",
        "json-schema": "^0.4.0",
        "mnemonist": "^0.39.7",
        "openai": "^4.4.0",
        "replicate": "^0.20.0"
    }
}
